{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run update:type && npm run migrate",
    "start": "node --max-old-space-size=4096 dist/app.js",
    "dev": "ts-node-dev --max-old-space-size=8192 app.ts",
    "migrate:gen": "prisma migrate dev --name ",
    "update:type": "prisma generate",
    "eslint:fix": "eslint --fix",
    "migrate": "prisma migrate deploy",
    "migrate:roll": "prisma migrate resolve --rolled-back ",
    "prebuild": "npm run eslint:fix",
    "build": "prisma generate && tsc --build tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-serve-static-core": "^5.0.0",
    "@types/node": "^22.7.6",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "google-auth-library": "^9.14.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.21.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "typescript-eslint": "^8.10.0"
  }
}
